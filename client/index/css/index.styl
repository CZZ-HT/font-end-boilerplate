
@import "../../common/css/normalize.css";
@import "../../common/css/common.styl";
@import "../../common/css/swiper.css";
.container {
	position: relative;
	padding-bottom: 1.35rem;
}

header {
	position: relative;
}

.swiper-wrapper,
.swiper-slide {
	height: auto;
}
.swiper-slide img {
	width: 100%;
}

.search-box {
    position: absolute;
	width: 100%;
	padding: 0.3125rem;
    top: 0;
    z-index: 9;
}

.search-box button {
    position: absolute;
    height: 0.5625rem;
    width: 0.5625rem;
    display: inline-block;
    left: 6%;
    bottom: 32%;
    border: none;
    background: url(/images/pages/022.png) no-repeat 0 0;
    background-size: 100% 100%;
}

.search-box input {
    width: 100%;
    height: 0.82rem;
    padding-left: 0.8rem;
    font-size: 0.375rem;
    border: none;
    color: #000;
    text-indent: .4rem;
    background-color: rgba(255, 255, 255, 0.8);
    border-radius: 0.8rem;
}

nav {
	position: relative;
	width: 100%;
	height: 1.171875rem;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	background-color: white;
}

nav > ul {
    float: left;
	width: 8.75rem;
    background-color: white;
    overflow: hidden;
}

nav > ul > li {
    float: left;
    min-width: 1.75rem;
    line-height: 1.171875rem;
    text-align: center;
}

nav > ul > li > a {
	color:#929697;
}

nav >  ul > li > a.active {
	color:#e21742;
	padding: .15rem;
	border-bottom: 1px solid #e21742;
}

nav .arrow {
    float: right;
	width: 1.25rem;
    height: 100%;
    background: white;
    border-left: 2px solid #eee;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

nav .arrow > a {
	display: block;
    width: 0.28125rem;
    height: 0.28125rem;
    border-left: 1px solid #818586;
    border-bottom: 1px solid #818586;
    margin: 0.5rem auto;
    -webkit-transform: rotate(135deg);
	transform: rotate(135deg);
}

nav .arrow > a.more {
	margin: 0.4rem auto;
	-webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);
}

.guid-box {
	display: none;
    position: absolute;
    width: 100%;
	top: 1.171875rem;
    left: 0;
    z-index: 10;
    background-color: #fbfbfb;
	-webkit-animation: fadeIn 0.1s linear both;
    animation: fadeIn 0.1s linear both;
}

.guid-box > a {
    float: left;
	display: block;
    width: 2.5rem;
    line-height: 0.78125rem;
    color: #000;
    font-size: 0.375rem;
    text-align: center;
    border-right: 1px solid #f1edec;
    border-bottom: 1px solid #f1edec;
    padding: 0.3125rem 0;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

.active-box {
	margin-top: 0.2rem;
    margin-bottom: 0.15rem;
    height: 1.6rem;
    line-height: 1.6rem;
	font-size: 0.5rem;
    text-align: center;
    color: #ef3353;
    background-color: #fff;
    word-break: break-all;
}

.active-box span {
	padding:0 0.5rem;
}

.goods-list {
	width: 100%;
	-webkit-box-sizing:border-box;
	box-sizing: border-box;
	overflow: hidden;
}

.goods-list li {
	position: relative;
    float: left;
	width: 4.921875rem;
    background-color: #fff;
	padding: 0 0 0.3125rem 0;
    margin-bottom: 0.15rem;
}

.goods-list li a {
	display: block;
    width: 100%;
    color: #4d4c4c;
    overflow: hidden;
}

.goods-list li a span {
	display: inline-block;
	width: 4.921875rem;
    height: 4.03125rem;
    overflow: hidden;
}

.goods-list li a b {
	display: inline-block;
	font-size: 0.375rem;
    color: #ff6666;
    font-weight: normal;
    font-style: normal;
    padding: 0 0.1rem;
    padding-right: .2rem;
}

.goods-list li a em {
	display: inline-block;
	font-size: 0.3125rem;
    color: #929697;
    font-style: normal;
    text-decoration: line-through;
}

.goods-list li a i{
	float: right;
	width: 1.09375rem;
	height: 0.4rem;
	line-height: 0.4rem;
	margin: .1rem .1rem 0 auto;
    background-color: #ef3353;
    border-radius: 0.09375rem;
    color: #fff;
    text-align: center;
    font-size: 0.25rem;
    font-style: normal;
}

.goods-list li:nth-of-type(2n+1) {
    margin-right: 0.15rem;
}

.goods-list li p {
	color: #1f282e;
    padding: 0 0.1rem;
    font-size: 0.375rem;
    overflow: hidden;
    height: 1rem;
    line-height: 0.5rem;
    margin-bottom: 0.25rem;
}
.more-link {
	display: block;
    width: 2.96875rem;
    height: 0.625rem;
    line-height: 0.625rem;
    margin: 0.25rem auto;
    text-align: center;
    color: #929697;
    font-size: 0.375rem;
    border-radius: 15px
}

.go-top {
	position: fixed;
    right: 0.3rem;
    bottom: 2rem;
    width: 1.09375rem;
}

.nav-bar {
	position: fixed;
    bottom: 0;
    width: 100%;
    z-index: 10;
    background-color: #fff;
    font-size: 0.28125rem;
    border-top: 1px solid #dcdcdc;
    overflow: hidden;
}

.nav-bar a {
    float: left;
    position: relative;
	width: 25%;
	color:black;
	height: 1.34375rem;
}

.nav-bar a i {
	width: 0.5625rem;
    height: 0.5625rem;
    position: absolute;
    left: 50%;
    top: 0.25625rem;
    margin-left: -0.28125rem;
    background-repeat: no-repeat;
    background-size: 100% 100%;
}

.icon-main {
    background-image: url(/images/pages/051.png);
}

.active .icon-main {
    background-image: url(/images/pages/051_red.png);
    background-size: 5.625rem 0.5625rem;
}
.icon-types-02 {
    background-image: url(/images/pages/goods-shelves.png);
}

.icon-mine {
    background-image: url(/images/pages/049.png);
}

.icon-shop {
    background-image: url(/images/pages/003.png);
}

.atips {
    display: block;
    width: 100%;
    height: auto;
    text-align: center;
    color: #7e7676;
    line-height: 0;
    margin-top: 1.12rem;
    font-size: 0.3125rem;
}